<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <%
  var title = page.title;

  if (is_archive()){
    title = 'Archives';

    if (is_month()){
      title += ': ' + page.year + '/' + page.month;
    } else if (is_year()){
      title += ': ' + page.year;
    }
  } else if (is_category()){
    title = 'Category: ' + page.category;
  } else if (is_tag()){
    title = 'Tag: ' + page.tag;
  }
  %>
  <title><% if (title){ %><%= title %> | <% } %><%= config.title %></title>
  <script>
    if (location.host.indexOf('cssha.com') >= 0) {
      location.href = location.href.replace('cssha.com', 'annn.me');
    }
  </script>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <meta property="wb:webmaster" content="35db845a3f9d1692" />
  <meta name="baidu-site-verification" content="VdeWu2EHCz" />
  <meta http-equiv="pragma" content="no-cache">
  <%- open_graph({twitter_id: theme.twitter, google_plus: theme.google_plus, fb_admins: theme.fb_admins, fb_app_id: theme.fb_app_id}) %>
  <% if (theme.rss){ %>
    <link rel="alternative" href="<%- url_for(theme.rss) %>" title="<%= config.title %>" type="application/rss+xm">
  <% } %>
  <% if (theme.favicon){ %>
    <link rel="icon" href="<%- url_for(theme.favicon) %>">
  <% } %>
  <link href="<%- url_for('css/fonts/source-code-pro.woff2') %>" rel="stylesheet" type="text/css">
  <%- css('css/style') %>
  <%- partial('analytics') %>
</head>